{
  "name": "vanilla-html",
  "version": "1.0.0",
  "description": "A template project for HTML, CSS and JavaScript including SASS, ESLint and Prettier.",
  "source": "./src/index.html",
  "scripts": {
    "dev": "npm-run-all --parallel bundle:* watch:sass",
    "bundle:eleventy": "eleventy --serve --quiet",
    "bundle:parcel": "parcel src/js/main.js src/assets/*",
    "watch:sass": "sass --watch src/scss:dist/css",
    "build:sass": "sass src/scss:dist/css",
    "build": "run-s prod:* build:sass",
    "prod:eleventy": "eleventy",
    "prod:parcel": "parcel build src/**/main.js src/assets/*"
  },
  "keywords": [
    "Vanilla",
    "HTML",
    "CSS",
    "JS",
    "JavaScript",
    "SASS",
    "SCSS"
  ],
  "author": "Matthew J Davison",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@html-eslint/eslint-plugin": "^0.11.0",
    "@html-eslint/parser": "^0.11.0",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "html-validate": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.4.0",
    "sassdoc": "^2.7.3"
  },
  "dependencies": {}
}
